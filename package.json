{
  "name": "media-tag",
  "version": "0.2.0",
  "main": "src/media-tag.js",
  "repository": "git@git.rnd.alterway.fr:UCF/media_tag_v2.git",
  "author": "UCF Project <ucf-project@alterway.fr>",
  "license": "Apache-2.0",
  "scripts": {
    "xo": "xo",
    "test": "xo && nyc ava",
    "build": "webpack",
    "start": "webpack-dev-server",
    "build:docs": "esdoc"
  },
  "esdoc": {
    "source": "./src/",
    "destination": "./dist/docs",
    "manual": {
      "design": [
        "./docs/proposal.md"
      ],
      "usage": [
        "./docs/integration.md"
      ],
      "changelog": [
        "./CHANGELOG.md"
      ]
    },
    "experimentalProposal": {
      "classProperties": true,
      "objectRestSpread": true,
      "decorators": true,
      "doExpressions": true,
      "functionBind": true,
      "asyncGenerators": true,
      "exportExtensions": true,
      "dynamicImport": true
    }
  },
  "xo": {
    "esnext": true,
    "ignores": [
      "dist/**/*.js",
      "demo/media-tag*.js"
    ],
    "overrides": [
      {
        "files": "demo/**",
        "esnext": false,
        "envs": [
          "browser"
        ]
      }
    ]
  },
  "dependencies": {
    "ava": "^0.18.2",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.0",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.2.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-runtime": "^6.23.0",
    "browser-env": "^2.0.25",
    "debug": "^2.6.1",
    "esdoc": "^0.5.2",
    "google-closure-compiler-js": "^20170124.0.0",
    "nyc": "^10.1.2",
    "tweetnacl": "^1.0.0-rc.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "xo": "^0.17.1",
    "yarn": "^0.21.3"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline",
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "ava": {
    "require": [
      "babel-register",
      "./test/helpers/setup-browser-env.js"
    ]
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false,
    "all": true,
    "reporter": [
      "html",
      "text"
    ],
    "report-dir": "./dist/coverage"
  }
}
