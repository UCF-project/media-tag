{
  "name": "media-tag",
  "version": "0.2.7",
  "main": "src/media-tag.js",
  "repository": "git@git.rnd.alterway.fr:UCF/media_tag_v2.git",
  "author": "UCF Project <ucf-project@alterway.fr>",
  "license": "Apache-2.0",
  "scripts": {
    "test": "xo && nyc _mocha ./test/media-tag.js",
    "build:dev": "webpack --config ./webpack.config/media-tag.webpack.config.dev.js --progress --colors",
    "build:pro": "webpack -p --config ./webpack.config/media-tag.webpack.config.pro.js --progress --colors",
    "build:amd": "babel --presets=react,es2015,stage-0 src/ -d babel/ && r.js -convert babel/ build/amd && rm -r babel/",
    "build:site": "webpack --config ./webpack.config/site.webpack.config.dev.js --progress --colors",
    "build:docs": "esdoc",
    "start": "yarn build:dev && cp -v -R dist/ site/www/assets/dist/ && webpack-dev-server --define process.env.NODE_ENV='development' --config ./webpack.config/site.webpack.config.dev.js --progress --colors",
    "xo": "xo",
    "karma": "karma start",
    "mocha": "nyc _mocha ./test/media-tag.js"
  },
  "esdoc": {
    "source": "./src/",
    "destination": "./dist/docs",
    "manual": {
      "design": [
        "./docs/proposal.md",
        "./docs/plugin.md",
        "./docs/filter.md"
      ],
      "usage": [
        "./docs/integration.md"
      ],
      "changelog": [
        "./CHANGELOG.md"
      ]
    },
    "experimentalProposal": {
      "classProperties": true,
      "objectRestSpread": true,
      "decorators": true,
      "doExpressions": true,
      "functionBind": true,
      "asyncGenerators": true,
      "exportExtensions": true,
      "dynamicImport": true
    }
  },
  "xo": {
    "esnext": true,
    "ignores": [
      "build/**/*",
      "dist/**/*",
      "demo/**/*",
      "test/**/*",
      "src/**/*/loading*.js",
      "src/**/algorithms/*.js",
      "src/react/**/*",
      "site/**/*",
      "karma.conf.js"
    ],
    "overrides": [
      {
        "files": "demo/**",
        "esnext": false,
        "envs": [
          "browser"
        ]
      }
    ]
  },
  "dependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.2.1",
    "babel-preset-stage-0": "^6.22.0",
    "babel-runtime": "^6.23.0",
    "browser-env": "^2.0.25",
    "chai": "^3.5.0",
    "debug": "^2.6.1",
    "esdoc": "^0.5.2",
    "file-saver": "^1.3.3",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "mocha": "^3.2.0",
    "nyc": "^10.1.2",
    "offline-js": "^0.7.19",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "requirejs": "^2.3.3",
    "tweetnacl": "^1.0.0",
    "tweetnacl-util": "^0.15.0",
    "yarn": "^0.21.3"
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-0"
    ],
    "env": {
      "production": {
        "sourceMaps": false
      },
      "development": {
        "sourceMaps": false,
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "require": [
      "babel-register",
      "./test/helpers/setup-browser-env.js"
    ],
    "exclude": [
      "demo/",
      "test/dependencies/",
      "src/react/",
      "site/**/*"
    ],
    "sourceMap": false,
    "instrument": false,
    "all": true,
    "reporter": [
      "html",
      "text"
    ],
    "report-dir": "./dist/coverage"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.4",
    "html-webpack-plugin": "^2.29.0",
    "react-mdl": "^1.10.3",
    "style-loader": "^0.18.2",
    "transfer-webpack-plugin": "^0.1.4",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.6.1",
    "xo": "^0.18.2"
  }
}
