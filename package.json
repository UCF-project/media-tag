{
  "name": "media-tag",
  "version": "0.2.6",
  "main": "src/media-tag.js",
  "repository": "git@git.rnd.alterway.fr:UCF/media_tag_v2.git",
  "author": "UCF Project <ucf-project@alterway.fr>",
  "license": "Apache-2.0",
  "scripts": {
    "test": "xo && nyc _mocha ./test/media-tag.js",
    "build": "webpack",
    "build:amd": "r.js -convert src/ temp/ && babel temp/ -d dist/amd-es5/ --presets=react,es2015,stage-0 && rm -r temp/",
    "build:docs": "esdoc",
    "build:amd-es5": "r.js -convert src/ temp/ && babel temp/ -d dist/amd-es5/ && rm -r temp/",
    "build:amd-es6": "r.js -convert src/ dist/amd-es6/",
    "start": "webpack-dev-server",
    "xo": "xo",
    "karma": "karma start",
    "mocha": "nyc _mocha ./test/media-tag.js"
  },
  "esdoc": {
    "source": "./src/",
    "destination": "./dist/docs",
    "manual": {
      "design": [
        "./docs/proposal.md",
        "./docs/plugin.md",
        "./docs/filter.md"
      ],
      "usage": [
        "./docs/integration.md"
      ],
      "changelog": [
        "./CHANGELOG.md"
      ]
    },
    "experimentalProposal": {
      "classProperties": true,
      "objectRestSpread": true,
      "decorators": true,
      "doExpressions": true,
      "functionBind": true,
      "asyncGenerators": true,
      "exportExtensions": true,
      "dynamicImport": true
    }
  },
  "xo": {
    "esnext": true,
    "ignores": [
      "dist/**/*",
      "demo/**/*",
      "test/**/*",
      "src/**/*/loading*.js"
    ],
    "overrides": [
      {
        "files": "demo/**",
        "esnext": false,
        "envs": [
          "browser"
        ]
      }
    ]
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.2.1",
    "babel-preset-stage-0": "^6.22.0",
    "babel-runtime": "^6.23.0",
    "browser-env": "^2.0.25",
    "chai": "^3.5.0",
    "debug": "^2.6.1",
    "esdoc": "^0.5.2",
    "file-saver": "^1.3.3",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "mocha": "^3.2.0",
    "nyc": "^10.1.2",
    "offline-js": "^0.7.19",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "requirejs": "^2.3.3",
    "yarn": "^0.21.3"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "env": {
      "development": {
        "sourceMaps": false,
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "require": [
      "babel-register",
      "./test/helpers/setup-browser-env.js"
    ],
    "exclude": [
      "demo/",
      "test/dependencies/"
    ],
    "sourceMap": false,
    "instrument": false,
    "all": true,
    "reporter": [
      "html",
      "text"
    ],
    "report-dir": "./dist/coverage"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.4",
    "html-webpack-plugin": "^2.29.0",
    "react-mdl": "^1.10.3",
    "style-loader": "^0.18.2",
    "transfer-webpack-plugin": "^0.1.4",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.6.1",
    "xo": "^0.18.2"
  }
}
